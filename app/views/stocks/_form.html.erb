
<%= simple_form_for(@stock) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :stock %>
    <%= f.input :symbol %>
    <%= f.input :first_sale %>
    <%= f.input :last_sale %>
    <%= f.input :sales %>
    <%= f.input :proceeds %>
    <%= f.input :cost %>
    <%= f.input :return %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
